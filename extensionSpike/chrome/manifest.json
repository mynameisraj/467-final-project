{
    "name": "467 Spike",
    "version": "0.1",
    "manifest_version": 2,
    "description": "Injecting script into a page using Chrome extension.",
    "browser_action": {
        "default_icon": "icon.png",
        "default_title" : "Test stuff"
    },
	"permissions": [
        "activeTab"
	],
    "background" : {
        "scripts" : ["script.js"],
        "persistent" : false
    },
    "web_accessible_resources": ["script.js"]
}
